<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Westside Boiler Invasion" %></title>

    <%= stylesheet_link_tag "normalize", "all" %>
    <%= javascript_include_tag  "all" %>
  </head>

  <body class="<%= page_classes %>">
	<div class="container">
		<div class="row">
			<div class="col-md-12">Fixed top Navbar</div>
		</div>
		<div class="row">
			<div class="container">
				<div class="row">
					<div class="col-md-12">Carousel</div>
					<div class="col-md-3">social media feed</div>
					<div class="col-md-9">content/blog posts</div>
				</div>
			</div>
		</div>
	</div>
	<%= yield %>
  </body>
</html>
